{
	"name": "primaria/pri-core",
    "description": "Extencion de principal del proyecto primaria",
    "keywords": ["yii2", "core", "primaria","pri-core"],
	"type": "yii2-extension",
	"license": "BSD-3-Clause",
	"support" : {
		"issues" : "https://github.com/primaria/pri-core/issues?state=open",
		"source" : "https://github.com/primaria/pri-core"
	},
	"authors": [{
		"name": "primaria",
		"email": "primaria@primaria.cl"
	}],
	"require": {
		"yiisoft/yii2": "~2.0.13",
		"yiisoft/yii2-bootstrap" : "^2.0.0",
		"yiisoft/yii2-swiftmailer" : "^2.0.0",
		"yiisoft/yii2-authclient" : "^2.1.0",
		"yiisoft/yii2-bootstrap" : "^2.0.0"
	},
	"require-dev" : {
		"yiisoft/yii2-codeception" : "^2.0.0",
		"codeception/specify" : "^0.4.3",
		"codeception/verify" : "^0.3.1"
	},
	"minimum-stability" : "dev",
	"autoload": {
		"psr-4": { "primaria\\core\\": "src" }
	},
	"extra" : {
		"bootstrap" : "primaria\\core\\components\\bootstrap\\ModuleAutoLoader",
		"branch-alias" : {
			"dev-master" : "0.1.x-dev"
		}
	}
}
